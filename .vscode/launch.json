{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Gulp task",
        "program": "${workspaceRoot}/node_modules/gulp/bin/gulp.js",
        "args": [
          "default"
        ]
      },
      {
        "type": "node",
        "request": "launch",
        "name": "Launch Program",
        "program": "${file}",
        "outFiles": [
          "${workspaceRoot}/**/*.js"
        ]
      }
    ]
  }